[binaries]
name = 'android'
c = '/opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/bin/aarch64-linux-android28-clang'
cpp = '/opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/bin/aarch64-linux-android28-clang++'
ar = '/opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/bin/aarch64-linux-android-ar'
ld = '/opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/bin/aarch64-linux-android-ld'
strip = '/opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/bin/aarch64-linux-android-strip'

[properties]
root = '/opt/android-ndk/toolchains/llvm/prebuilt/linux-x86_64/sysroot'
swa_android_buildtools = '/opt/android-sdk/build-tools/29.0.2'
swa_android_platform = '/opt/android-sdk/platforms/android-28'

[host_machine]
system = 'android'
cpu_family = 'aarch64'
cpu = 'arm64-v8a'
endian = 'big' # not sure
